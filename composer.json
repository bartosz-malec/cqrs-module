{
    "name": "pauci/cqrs-module",
    "description": "Zend Framework 2 Module that provides functionality for CQRS library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^5.6 || ^7.0",
        "pauci/cqrs": "dev-master"
    },
    "require-dev": {
        "zendframework/zendframework": "^2.5.2",
        "zendframework/zend-developer-tools": "*",
        "doctrine/doctrine-orm-module": "^0.9.2",
        "phpunit/phpunit": "^5.1.4"
    },
    "autoload": {
        "psr-4": {
            "CQRSModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CQRSModuleTest\\": "tests/CQRSModuleTest/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
