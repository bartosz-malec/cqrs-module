{
    "name": "sygic/cqrs-module",
    "description": "Zend Framework 2 Module that provides functionality for CQRS library",
    "type": "library",
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "http://satis.ssd.sygic.local"
        }
    ],
    "require": {
        "php": ">=5.5",
        "sygic/cqrs": "*"
    },
    "require-dev": {
        "zendframework/zendframework": "2.*",
        "zendframework/zend-developer-tools": "*",
        "doctrine/doctrine-orm-module": "0.*",
        "phpunit/phpunit": "~4.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "CQRSModule\\": "src/"
        }
    }
}
